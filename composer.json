{
    "name": "nonetallt/jinitialize",
    "description": "Modular and extensible automation scripts in php.",
    "keywords": ["automation", "scripts", "project", "setup", "jinitialize", "modular", "extensible"],
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "nonetallt/jinitialize-core": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^6",
        "consolidation/robo": "~1",
        "henrikbjorn/lurker": "^1.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jyri Mikkola",
            "email": "jyri.mikkola@pp.inet.fi"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { 
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Nonetallt\\Jinitialize\\ComposerScripts::postAutoloadDump"
        ]
    }
}
